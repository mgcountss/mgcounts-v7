<style>
body {
    background-color: #242425;
    font-family: Arial, Helvetica, sans-serif;
    text-align: center;
    color: #FFF;
}

img {
    vertical-align: middle;
}

#nav {
    float: left;
    background-color: #000;
    position: relative;
    display: flex;
    width: 100%;
    padding-top: 0.2vw;
}

#nav>a>img {
    width: 15vw;
    height: 2.3vw;
}

#banner {
    text-align: left;
    width: 100%;
    height: 11vw;
    background-color: #000;
    background-repeat: no-repeat;
    background-size: cover;
    background-position: center;
}

.round {
    margin-left: 1vw;
    margin-top: 0.3vw;
    width: 10vw;
    height: 10vw;
    border-radius: 50%;
    border: 0.3vw solid #fff;
    background-color: #000;
    float: left;
    object-fit: cover;
}

.rect {
    margin-left: 1vw;
    margin-top: 0.3vw;
    width: 15vw;
    height: 10vw;
    border: 0.3vw solid #fff;
    background-color: #000;
    float: left;
    object-fit: cover;
}

#main {
    margin: auto;
    margin-top: 3vw;
    width: 90%;
    background-color: #1a1a1a;
}

#name {
    font-size: 3vw;
    margin-top: 0vw;
    font-weight: bold;
}

#count {
    font-size: 6vw;
    margin-top: -2vw;
}

#label {
    font-size: 2vw;
    margin-top: -3vw;
}

.sideCounts {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
}

.label {
    font-size: 3vw;
}

.label2 {
    font-size: 1vw;
}

.side {
    margin-top: -0.5vw;
    font-size: 4vw;
}

.canvas-wrap {
    position: relative;
    width: 12vw;
    height: 12vw;
}

#embed {
    width: 99%;
    font-size: 2vw;
}

#settings {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
}

.setting_title {
    font-size: 2vw;
    font-weight: bold;
    margin-top: 0.5vw;
}

.setting_input {
    height: 2vw;
    width: 5vw;
}

.setting_input2 {
    width: 15vw;
    height: 2vw;
    font-size: 1vw;
}

.setting_input3 {
    font-size: 2.5vw;
}

.setting_input3:hover {
    cursor: pointer;
}

table {
    border: solid #FFF;
    width: 90%;
    text-align: center;
    font-size: 1.5vw;
}
</style>